{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ESNext",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "#eslint-merge": [
        "index.mjs"
      ],
      "#eslint-merge/get-map-merge": [
        "get-map-merge.mjs"
      ],
      "#eslint-merge/merge": [
        "merge.mjs"
      ]
    }
  },
  "include": [
    "."
  ]
}
